<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ztp2.view.ProductEditViewController">
    <TextField fx:id="nameField" promptText="Product Name" />
    <TextField fx:id="priceField" promptText="Price" />
    <TextField fx:id="quantityField" promptText="Quantity" />
    <Button fx:id="editButton" text="Edit" />
    <Button fx:id="backButton" text="Return to Product List" />
</VBox>